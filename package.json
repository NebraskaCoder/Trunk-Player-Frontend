    "@tailwindcss/forms": "^0.5.4",
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "eslint": "^8.45.0",
    "eslint-config-next": "^13.4.10",
    "lodash": "^4.17.21",
    "next": "^13.4.10",
    "next-auth": "^4.22.3",
    "next-intl": "^3.0.0-beta.8",
