    "@types/cookie": "^0.5.1",
    "@types/luxon": "^2.3.2",
    "@types/react": "^18.0.14",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.29.0",
