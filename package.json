{
  "private": true,
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:export": "next build && next export",
    "export": "next export",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coveragereport": "jest --coverage --coverageDirectory coverage --coverageReporters lcov",
    "cy:open": "cypress open",
    "cy:run": "cypress run --config video=false",
    "cy:report": "cypress run report --config video=false",
    "test:e2e": "start-server-and-test dev http://localhost:3000 cy:open",
    "test:e2e:ci": "start-server-and-test dev http://localhost:3000 cy:run",
    "test:e2e:report": "start-server-and-test dev http://localhost:3000 cy:report"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "dependencies": {
    "@headlessui/react": "^1.6.1",
    "@heroicons/react": "^1.0.5",
    "@reduxjs/toolkit": "^1.6.2",
    "@sentry/nextjs": "^6.17.4",
    "@tailwindcss/forms": "^0.3.4",
    "axios": "^0.25.0",
    "cookie": "^0.4.2",
    "dayjs": "^1.10.7",
    "luxon": "^2.0.2",
    "next": "^12.0.7",
    "pre-commit": "^1.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.26.0",
    "react-loading-skeleton": "^3.0.3",
    "react-redux": "^7.2.6",
    "swr": "^1.2.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.1.6",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.2",
    "@types/cookie": "^0.4.1",
    "@types/luxon": "^2.0.5",
    "@types/react": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.19.0",
    "autoprefixer": "^10.3.7",
    "babel-jest": "^27.3.1",
    "cypress": "^9.5.1",
    "eslint": "^8.10.0",
    "eslint-config-async": "^1.0.1",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "react-test-renderer": "^17.0.2",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^2.2.17",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.4.4"
  }
