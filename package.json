    "luxon": "^2.4.0",
    "next": "^12.2.3",
    "pre-commit": "^1.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.33.0",
    "react-loading-skeleton": "^3.1.0",
