    "@types/lodash": "^4.14.195",
    "autoprefixer": "^10.4.14",
    "husky": "^8.0.0",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "jest-mock-axios": "^4.7.2",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.26",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.3.3"
  }
