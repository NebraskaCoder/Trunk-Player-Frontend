{
  "private": true,
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:export": "next build && next export",
    "export": "next export",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coveragereport": "jest --coverage --coverageDirectory coverage --coverageReporters lcov",
    "cy:open": "cypress open",
    "cy:run": "cypress run --config video=false",
    "cy:report": "cypress run report --config video=false",
    "test:e2e": "start-server-and-test dev http://localhost:3000 cy:open",
    "test:e2e:ci": "start-server-and-test dev http://localhost:3000 cy:run",
    "test:e2e:report": "start-server-and-test dev http://localhost:3000 cy:report"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "dependencies": {
