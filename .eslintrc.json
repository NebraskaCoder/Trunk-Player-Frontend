{
  "extends": ["eslint:recommended", "next/core-web-vitals"],
  "env": {
    "node": true,
    "jest": true
  },
  "globals": {
    "React": true
  },
  "rules": {
    "no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "^_"
      }
    ],
    "no-empty-function": "warn",
    "no-empty-pattern": "warn"
  }
}
